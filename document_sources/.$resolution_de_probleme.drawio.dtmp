        <mxCell id="NKkf0DxXOLrTYv5vM8v2-60" value="Cycle de résolution de problèmes" style="rounded=1;whiteSpace=wrap;html=1;fontSize=15;arcSize=23;strokeWidth=2;fillColor=#f5f5f5;strokeColor=#666666;fontColor=#333333;" parent="1" vertex="1">
